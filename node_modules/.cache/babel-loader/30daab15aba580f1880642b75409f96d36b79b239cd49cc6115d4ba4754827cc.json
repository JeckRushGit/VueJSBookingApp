{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      email: '',\n      password: '',\n      offline: false\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      try {\n        const response = await axios.post('ServletLogin', null, {\n          params: {\n            email: this.email,\n            password: this.password,\n            action: 'web'\n          },\n          withCredentials: true\n        });\n        let attributes = response.data.session.attributes;\n        localStorage.setItem('session', JSON.stringify(attributes));\n        this.$store.commit('setInit', attributes);\n        this.$router.push('/');\n      } catch (e) {\n        console.log(\"ciao\");\n        if (e.response) {\n          console.error(e);\n        } else {\n          this.offline = true;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","offline","methods","handleSubmit","response","post","params","action","withCredentials","attributes","session","localStorage","setItem","JSON","stringify","$store","commit","$router","push","e","console","log","error"],"sources":["C:\\Users\\giaco\\Desktop\\folderperview\\src\\pages\\LoginPage.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"!offline\">\r\n    <div class=\"d-flex flex-row\">\r\n      <div class=\"my-spacer\"></div>\r\n      <div class=\"inner-form d-flex mt-5 justify-content-center flex-column\">\r\n        <h1 style=\"text-align: center\">Sign in</h1>\r\n        <form @submit.prevent=\"handleSubmit\">\r\n          <!--      campo email-->\r\n          <div class=\"mb-3\">\r\n            <label for=\"input1\" class=\"form-label\">Email Address</label>\r\n            <input type=\"email\" required=\"true\" class=\"form-control\" id=\"input1\" placeholder=\"name@example.com\"\r\n                   v-model=\"email\">\r\n          </div>\r\n         