{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { toRaw } from \"vue\";\nimport AgendaLessonContainer from \"@/components/AgendaLessonContainer.vue\";\nimport FeedBackPopUp from \"@/components/FeedBackPopUp.vue\";\nimport AgendaPopUp from \"@/components/AgendaPopUp.vue\";\nexport default {\n  name: \"AgendaPage\",\n  components: {\n    AgendaPopUp,\n    FeedBackPopUp,\n    AgendaLessonContainer\n  },\n  data() {\n    return {\n      state: 1,\n      listOfReservedLessons: null,\n      dataLogic: null,\n      isPopUpOpen: false,\n      isFeedBackOpen: false,\n      dataToSend: null\n    };\n  },\n  async created() {\n    await this.fetchData();\n  },\n  methods: {\n    async fetchData() {\n      try {\n        const response = await axios.get('ServletGetBookingsForUser', {\n          params: {\n            action: 'web',\n            stato: toRaw(this.state)\n          }\n        });\n        this.listOfReservedLessons = response.data;\n      } catch (e) {\n        console.error(e);\n        if (e.response.status === 401) {\n          this.$handle_session_expired();\n        }\n      }\n    },\n    createLogicFromData(list) {\n      if (list.length > 0) {\n        const obj = {\n          days: {}\n        };\n        for (let i = 0; i < list.length; i++) {\n          obj.days[list[i].day + \"/\" + list[i].month] = {\n            lessons: []\n          };\n        }\n        for (let i = 0; i < list.length; i++) {\n          let listTmp = obj.days[list[i].day + \"/\" + list[i].month].lessons;\n          listTmp.push(list[i]);\n        }\n        this.dataLogic = obj;\n        console.log(obj);\n      }\n    },\n    handleSubmitLesson(index, item) {\n      this.isPopUpOpen = true;\n      this.dataToSend = item;\n    },\n    closePopUp() {\n      this.isPopUpOpen = false;\n    },\n    opencloseFeedBack(flag) {\n      this.isFeedBackOpen = flag;\n    },\n    async SendData(state) {\n      try {\n        let response;\n        let dataToSend = toRaw(this.dataToSend);\n        if (state === 2) {\n          response = axios.post('ServletSetBookings', null, {\n            params: {\n              action: 'web',\n              email_professore: dataToSend.professor.email,\n              corso: dataToSend.course.course_titol,\n              giorno: dataToSend.day,\n              mese: dataToSend.month,\n              orario: dataToSend.hour,\n              stato: state\n            }\n          });\n        } else if (state === 3) {\n          response = axios.post('ServletSetBookings', null, {\n            params: {\n              action: 'web',\n              email_professore: dataToSend.professor.email,\n              corso: dataToSend.course.course_titol,\n              giorno: dataToSend.day,\n              mese: dataToSend.month,\n              orario: dataToSend.hour,\n              stato: state\n            }\n          });\n        }\n        await this.fetchData();\n      } catch (e) {\n        console.error(e.response);\n      }\n    }\n  },\n  watch: {\n    listOfReservedLessons(newList, oldList) {\n      this.createLogicFromData(toRaw(newList));\n    }\n  },\n  computed: {\n    orderListOfDays() {\n      return Object.keys(this.dataLogic.days).sort();\n    }\n  }\n};","map":{"version":3,"names":["axios","toRaw","AgendaLessonContainer","FeedBackPopUp","AgendaPopUp","name","components","data","state","listOfReservedLessons","dataLogic","isPopUpOpen","isFeedBackOpen","dataToSend","created","fetchData","methods","response","get","params","action","stato","e","console","error","status","$handle_session_expired","createLogicFromData","list","length","obj","days","i","day","month","lessons","listTmp","push","log","handleSubmitLesson","index","item","closePopUp","opencloseFeedBack","flag","SendData","post","email_professore","professor","email","corso","course","course_titol","giorno","mese","orario","hour","watch","newList","oldList","computed","orderListOfDays","Object","keys","sort"],"sources":["C:\\Users\\giaco\\Desktop\\folderperview\\src\\pages\\AgendaPage.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"dataLogic\" v-for=\"(item,index) in orderListOfDays \">\r\n    <div class=\"my-container m-2 overflow-auto\">\r\n      <p>{{ item }} </p>\r\n      <p v-for=\"(item2,index2) in dataLogic.days[item].lessons\">\r\n        <AgendaLessonContainer :index=\"index2\" :item=\"item2\"\r\n                               :call-back-lesson=\"handleSubmitLesson\"></AgendaLessonContainer>\r\n      </p>\r\n    </div>\r\n  </div>\r\n  <AgendaPopUp v-if=\"isPopUpOpen\" :call-back-close-pop-up=\"closePopUp\" :call-back-handle-send-data=\"SendData\"/>\r\n  <FeedBackPopUp v-if=\"isFeedBackOpen\" :teleport-tag=\"'#agendapopup'\"\r\n                 :call-back-close-pop-up=\"opencloseFeedBack\"></FeedBackPopUp>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport {toRaw} from \"vue\";\r\nimport AgendaLessonContainer from \"@/components/AgendaLessonContainer.vue\";\r\nimport FeedBackPopUp from \"@/components/FeedBackPopUp.vue\";\r\nimport AgendaPopUp from \"@/components/AgendaPopUp.vue\";\r\n\r\nexport default {\r\n  name: \"AgendaPage\",\r\n  components: {AgendaPopUp, FeedBackPopUp, AgendaLessonContainer},\r\n  data() {\r\n    return {\r\n      state: 1,\r\n      listOfReservedLessons: null,\r\n      dataLogic: null,\r\n      isPopUpOpen: false,\r\n      isFeedBackOpen: false,\r\n      dataToSend: null,\r\n    }\r\n  },\r\n\r\n  async created() {\r\n    await this.fetchData();\r\n  },\r\n\r\n  methods: {\r\n    async fetchData() {\r\n      try {\r\n        const response = await axios.get('ServletGetBookingsForUser', {\r\n          params: {\r\n            action: 'web',\r\n            stato: toRaw(this.state)\r\n          }\r\n        })\r\n        this.listOfReservedLessons = response.data;\r\n      } catch (e) {\r\n        console.error(e);\r\n        if (e.response.status === 401) {\r\n          this.$handle_session_expired()\r\n        }\r\n      }\r\n    },\r\n\r\n    createLogicFromData(list) {\r\n      if (list.length > 0) {\r\n        const obj = {days: {}};\r\n        for (let i = 0; i < list.length; i++) {\r\n          obj.days[list[i].day + \"/\" + list[i].month] = {lessons: []};\r\n        }\r\n        for (let i = 0; i < list.length; i++) {\r\n          let listTmp = obj.days[list[i].day + \"/\" + list[i].month].lessons;\r\n          listTmp.push(list[i]);\r\n        }\r\n        this.dataLogic = obj;\r\n        console.log(obj);\r\n      }\r\n    },\r\n\r\n\r\n    handleSubmitLesson(index, item) {\r\n      this.isPopUpOpen = true;\r\n      this.dataToSend = item;\r\n    },\r\n\r\n    closePopUp() {\r\n      this.isPopUpOpen = false;\r\n    },\r\n\r\n    opencloseFeedBack(flag) {\r\n      this.isFeedBackOpen = flag;\r\n    },\r\n\r\n    async SendData(state) {\r\n      try {\r\n        let response;\r\n        let dataToSend = toRaw(this.dataToSend);\r\n        if (state === 2) {\r\n          response = axios.post('ServletSetBookings',null,{params : {action : 'web',email_professore : dataToSend.professor.email,corso : dataToSend.course.course_titol,giorno : dataToSend.day,mese : dataToSend.month,orario : dataToSend.hour,stato : state,}});\r\n        } else if (state === 3) {\r\n          response = axios.post('ServletSetBookings',null,{params : {action : 'web',email_professore : dataToSend.professor.email,corso : dataToSend.course.course_titol,giorno : dataToSend.day,mese : dataToSend.month,orario : dataToSend.hour,stato : state,}})\r\n        }\r\n        await this.fetchData();\r\n      } catch (e) {\r\n        console.error(e.response)\r\n      }\r\n    }\r\n\r\n\r\n  },\r\n\r\n  watch: {\r\n    listOfReservedLessons(newList, oldList) {\r\n      this.createLogicFromData(toRaw(newList));\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    orderListOfDays() {\r\n      return Object.keys(this.dataLogic.days).sort();\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n\r\n.my-container {