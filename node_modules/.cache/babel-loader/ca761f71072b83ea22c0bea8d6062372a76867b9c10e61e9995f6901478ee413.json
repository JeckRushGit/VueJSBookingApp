{"ast":null,"code":"import VueMultiselect from 'vue-multiselect';\nimport axios from \"axios\";\nexport default {\n  name: \"AdminRemoveLessons\",\n  components: {\n    VueMultiselect\n  },\n  async created() {\n    await this.fetchUsers();\n  },\n  data() {\n    return {\n      users: null,\n      selectedUser: null\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get('ServletAdminGetBookings', {\n          params: {\n            action: 'getListOfUsers',\n            device: 'web'\n          }\n        });\n        this.users = response.data;\n        console.log(response.data);\n      } catch (e) {\n        console.error(e.response);\n        if (e.response.status === 401) {\n          this.$handle_session_expired();\n        }\n      }\n    },\n    async fetchBookingsForUser(selectedUser) {\n      try {\n        const response = await axios.get('ServletAdminGetBookings', {\n          params: {\n            action: 'getBookingsForUser',\n            device: 'web',\n            userEmail: selectedUser.email\n          }\n        });\n        console.log(response.data);\n      } catch (e) {\n        console.error(e.response);\n        if (e.response.status === 401) {\n          this.$handle_session_expired();\n        }\n      }\n    }\n  },\n  computed: {\n    userData() {\n      return this.users;\n    }\n  },\n  watch: {\n    users(newUsers, oldUsers) {\n      this.selectedUser = newUsers[0];\n    },\n    selectedUser(newSelectedUser, oldSelectedUser) {\n      this.fetchBookingsForUser(newSelectedUser);\n    }\n  }\n};","map":{"version":3,"names":["VueMultiselect","axios","name","components","created","fetchUsers","data","users","selectedUser","methods","response","get","params","action","device","console","log","e","error","status","$handle_session_expired","fetchBookingsForUser","userEmail","email","computed","userData","watch","newUsers","oldUsers","newSelectedUser","oldSelectedUser"],"sources":["C:\\Use