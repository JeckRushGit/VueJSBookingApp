{"ast":null,"code":"import axios from \"axios\";\nimport { toRaw } from \"vue\";\nexport default {\n  name: \"AgendaPage\",\n  data() {\n    return {\n      state: 1,\n      listOfReservedLessons: null,\n      dataLogic: null\n    };\n  },\n  async created() {\n    await this.fetchData();\n  },\n  methods: {\n    async fetchData() {\n      try {\n        const response = await axios.get('ServletGetBookingsForUser', {\n          params: {\n            action: 'web',\n            stato: toRaw(this.state)\n          }\n        });\n        this.listOfReservedLessons = response.data;\n      } catch (e) {\n        console.error(e);\n      }\n    },\n    createLogicFromData(list) {\n      if (list.length > 0) {\n        const obj = {\n          days: {}\n        };\n        for (let i = 0; i < list.length; i++) {\n          obj.days[list[i].day + \"/\" + list[i].month] = {\n            lessons: []\n          };\n        }\n        for (let i = 0; i < list.length; i++) {\n          obj.days[list[i].day + \"/\" + list[i].month] = {\n            lessons: []\n          };\n          let list = obj.days[list[i].day + \"/\" + list[i].month];\n          list.add();\n        }\n      }\n    }\n  },\n  watch: {\n    listOfReservedLessons(newList, oldList) {\n      console.log(toRaw(newList));\n      this.createLogicFromData(newList);\n    }\n  },\n  computed: {\n    test: function () {\n      return this.listOfReservedLessons.map(function (item) {\n        return item.professor;\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","toRaw","name","data","state","listOfReservedLessons","dataLogic","created","fetchData","methods","response","get","params","action","stato","e","console","error","createLogicFromData","list","length","obj","days","i","day","month","lessons","add","watch","newList","oldList","log","computed","test","map","item","professor"],"sources":["C:\\Users\\giaco\\Desktop\\folderperview\\src\\pages\\AgendaPage.vue"],"sourcesContent":["<template>\r\n  <bu