()\r\n        }\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    userData(){\r\n      return this.users.map(function (user){\r\n        return {label : user.name+\" \"+user.surname+\" (\"+user.email+\")\",info : user};\r\n      });\r\n    },\r\n    listOfLessonsLogic(){\r\n      return this.listOfLessons.reduce((dates,info) => {\r\n        if(!dates[info.day]){\r\n          dates[info.day] = [];\r\n        }\r\n        dates[info.day].push(info);\r\n        return dates;\r\n      },{});\r\n\r\n\r\n\r\n    }\r\n  },\r\n  watch: {\r\n    users(newUsers, oldUsers) {\r\n      this.selectedUser = this.userData[0];\r\n    },\r\n    async selectedUser(newSelectedUser, oldSelectedUser) {\r\n      await this.fetchBookingsForUser(newSelectedUser.info);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAwC;gCACjDC,mBAAA,CAAyB,YAArB,kBAAgB;;EACfC,KAAsC,EAAtC;IAAA;IAAA;EAAA;AAAsC;;;6DAF7CD,mBAAA,CAKM,OALNE,UAKM,GAJJC,UAAyB,EACzBH,mBAAA,CAEM,OAFNI,UAEM,GADkBC,KAAA,CAAAC,KAAK,I,cAA3BC,YAAA,CAAuFC,yBAAA;;IAA1DC,KAAK,EAAC,OAAO;IAAEC,OAAO,EAAEC,QAAA,CAAAC,QAAQ;gBAAWP,KAAA,CAAAQ,YAAY;+DAAZR,KAAA,CAAAQ,YAAY,GAAAC,MAAA;+FAG7ET,KAAA,CAAAU,aAAa,I,kBAAxBC,mBAAA,CAEMC,SAAA;IAAAC,GAAA;EAAA,GAAAC,WAAA,CAFkCC,MAAM,CAACC,IAAI,CAACV,QAAA,CAAAW,kBAAkB,GAArCJ,GAAG;yBAApCF,mBAAA,CAEM,aAAAO,gBAAA,CADFL,GAAG,IAAE,KACT"},"metadata":{},"sourceType":"module","externalDependencies":[]}