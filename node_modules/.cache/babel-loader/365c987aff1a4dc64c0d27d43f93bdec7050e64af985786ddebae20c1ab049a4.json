{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport VueMultiselect from 'vue-multiselect';\nimport axios from \"axios\";\nimport { toRaw } from \"vue\";\nimport Icon from \"@/components/Icon.vue\";\nexport default {\n  name: \"AdminRemoveLessons\",\n  components: {\n    Icon,\n    VueMultiselect\n  },\n  async created() {\n    await this.fetchUsers();\n  },\n  data() {\n    return {\n      users: null,\n      selectedUser: null,\n      listOfLessons: null\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get('ServletAdminGetBookings', {\n          params: {\n            action: 'getListOfUsers',\n            device: 'web'\n          }\n        });\n        this.users = response.data;\n      } catch (e) {\n        console.error(e.response);\n        if (e.response.status === 401) {\n          this.$handle_session_expired();\n        }\n      }\n    },\n    async fetchBookingsForUser(selectedUser) {\n      try {\n        const response = await axios.get('ServletAdminGetBookings', {\n          params: {\n            action: 'getBookingsForUser',\n            device: 'web',\n            userEmail: selectedUser.email\n          }\n        });\n        this.listOfLessons = response.data;\n      } catch (e) {\n        console.error(e.response);\n        if (e.response.status === 401) {\n          this.$handle_session_expired();\n        }\n      }\n    }\n  },\n  computed: {\n    userData() {\n      return this.users.map(function (user) {\n        return {\n          label: user.name + \" \" + user.surname + \" (\" + user.email + \")\",\n          info: user\n        };\n      });\n    },\n    listOfLessonsLogic() {\n      return this.listOfLessons.reduce((dates, info) => {\n        if (!dates[info.day + '/' + info.month]) {\n          dates[info.day + '/' + info.month] = [];\n        }\n        dates[info.day + '/' + info.month].push(info);\n        return dates;\n      }, {});\n    }\n  },\n  watch: {\n    users(newUsers, oldUsers) {\n      this.selectedUser = this.userData[0];\n    },\n    async selectedUser(newSelectedUser, oldSelectedUser) {\n      await this.fetchBookingsForUser(newSelectedUser.info);\n    }\n  }\n};","map":{"version":3,"names":["VueMultiselect","axios","toRaw","Icon","name","components","created","fetchUsers","data","users","selectedUser","listOfLessons","methods","response","get","params","action","device","e","console","error","status","$handle_session_expired","fetchBookingsForUser","userEmail","email","computed","userData","map","user","label","surname","info","listOfLessonsLogic","reduce","dates","day","month","push","watch","newUsers","oldUsers","newSelectedUser","oldSelectedUser"],"sources":["C:\\Users\\giaco\\Desktop\\folderperview\\src\\pages\\AdminRemoveLessons.vue"],"sourcesContent":["<template>\r\n  <div class=\"m-5 d-flex flex-row align-items-center\">\r\n    <h3>List of users : </h3>\r\n    <div style=\"width: 400px;margin-left: 20px\">\r\n      <VueMultiselect v-if=\"users\" label=\"label\" :options=\"userData\" v-model=\"selectedUser\"/>\r\n    </div>\r\n  </div>\r\n<!--  <div v-if=\"listOfLessons\" v-for=\"key in Object.keys(listOfLessonsLogic).sort()\">-->\r\n<!--    {{key}} : {{listOfLessonsLogic[key]}}-->\r\n<!--  </div>-->\r\n\r\n\r\n  <div v-if=\"listOfLessons\" class=\"accordion\" id=\"accordionExample\" v-for=\"(item,index) in Object.keys(listOfLessonsLogic).sort()\">\r\n    <div class=\"accordion-item mt-2\" >\r\n      <h2 class=\"accordion-header\" id=\"headingOne\">\r\n        <button class=\"accordion-button\" type=\"button\" data-bs-toggle=\"collapse\" :data-bs-target=\"'#collapse'+index\" aria-expanded=\"true\" aria-controls=\"collapseOne\">\r\n          {{item}}\r\n        </button>\r\n      </h2>\r\n      <div :id=\"'collapse'+index\" class=\"accordion-collapse collapse \" aria-labelledby=\"headingOne\" data-bs-parent=\"#accordionExample\">\r\n        <div class=\"accordion-body\">\r\n          <div class=\"d-flex flex-row justify-content-between\" v-for=\"item2 in listOfLessonsLogic[item]\">\r\n            <p> - {{item2.hour}} : {{item2.course.course_titol}} , {{item2.professor.name}} {{item2.professor.surname}}  ({{item2.professor.email}})</p>\r\n          <button  @click=\"handleClick(3)\" class=\"m-1 btn btn-danger btn-circle\">\r\n            <Icon name=\"trash\"/>\r\n          </button>\r\n            <hr>\r\n          </div>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport VueMultiselect from 'vue-multiselect'\r\nimport axios from \"axios\";\r\nimport {toRaw} from \"vue\";\r\nimport Icon from \"@/components/Icon.vue\";\r\n\r\nexport default {\r\n  name: \"AdminRemoveLessons\",\r\n  components: {\r\n    Icon,\r\n    VueMultiselect\r\n  },\r\n\r\n  async created() {\r\n    await this.fetchUsers();\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      users: null,\r\n      selectedUser: null,\r\n      listOfLessons : null,\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchUsers() {\r\n      try {\r\n        const response = await axios.get('ServletAdminGetBookings', {\r\n          params: {\r\n            action: 'getListOfUsers',\r\n            device: 'web'\r\n          }\r\n        });\r\n        this.users = response.data;\r\n\r\n      } catch (e) {\r\n        console.error(e.response);\r\n        if (e.response.status === 401) {\r\n          this.$handle_session_expired()\r\n        }\r\n      }\r\n    },\r\n\r\n    async fetchBookingsForUser(selectedUser) {\r\n      try {\r\n        const response = await axios.get('ServletAdminGetBookings', {\r\n          params: {\r\n            action: 'getBookingsForUser',\r\n            device: 'web',\r\n            userEmail: selectedUser.email\r\n          }\r\n        });\r\n        this.listOfLessons = response.data;\r\n      } catch (e) {\r\n        console.error(e.response);\r\n        if (e.response.status === 401) {\r\n          this.$handle_session_expired()\r\n        }\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    userData(){\r\n      return this.users.map(function (user){\r\n        return {label : user.name+\" \"+user.surname+\" (\"+user.email+\")\",info : user};\r\n      });\r\n    },\r\n    listOfLessonsLogic(){\r\n      return this.listOfLessons.reduce((dates,info) => {\r\n        if(!dates[info.day+'/'+info.month]){\r\n          dates[info.day+'/'+info.month] = [];\r\n        }\r\n        dates[info.day+'/'+info.month].push(info);\r\n        return dates;\r\n      },{});\r\n    }\r\n  },\r\n  watch: {\r\n    users(newUsers, oldUsers) {\r\n      this.selectedUser = this.userData[0];\r\n    },\r\n    async selectedUser(newSelectedUser, oldSelectedUser) {\r\n      await this.fetchBookingsForUser(newSelectedUser.info);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.btn-circle {\r\n  width: 30px;\r\n  height: 30px;\r\n  padding: 0px 0px;\r\n  border-radius: 20px;\r\n  font-size: 8px;\r\n  text-align: center;\r\n}\r\n</style>"],"mappings":";AA2CA,OAAOA,cAAa,MAAO,iBAAgB;AAC3C,OAAOC,KAAI,MAAO,OAAO;AACzB,SAAQC,KAAK,QAAO,KAAK;AACzB,OAAOC,IAAG,MAAO,uBAAuB;AAExC,eAAe;EACbC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IACVF,IAAI;IACJH;EACF,CAAC;EAED,MAAMM,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,UAAU,EAAE;EACzB,CAAC;EAEDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI;MACXC,YAAY,EAAE,IAAI;MAClBC,aAAY,EAAI;IAClB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAML,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMM,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,yBAAyB,EAAE;UAC1DC,MAAM,EAAE;YACNC,MAAM,EAAE,gBAAgB;YACxBC,MAAM,EAAE;UACV;QACF,CAAC,CAAC;QACF,IAAI,CAACR,KAAI,GAAII,QAAQ,CAACL,IAAI;MAE5B,EAAE,OAAOU,CAAC,EAAE;QACVC,OAAO,CAACC,KAAK,CAACF,CAAC,CAACL,QAAQ,CAAC;QACzB,IAAIK,CAAC,CAACL,QAAQ,CAACQ,MAAK,KAAM,GAAG,EAAE;UAC7B,IAAI,CAACC,uBAAuB,EAAC;QAC/B;MACF;IACF,CAAC;IAED,MAAMC,oBAAoBA,CAACb,YAAY,EAAE;MACvC,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,yBAAyB,EAAE;UAC1DC,MAAM,EAAE;YACNC,MAAM,EAAE,oBAAoB;YAC5BC,MAAM,EAAE,KAAK;YACb