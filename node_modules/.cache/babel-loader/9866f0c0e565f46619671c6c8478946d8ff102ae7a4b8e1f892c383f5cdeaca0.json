: {\r\n    async fetchData() {\r\n      try {\r\n        const response = await axios.get('ServletGetBookingsForUser', {\r\n          params: {\r\n            action: 'web',\r\n            stato: toRaw(this.state)\r\n          }\r\n        })\r\n        this.listOfReservedLessons = response.data;\r\n      } catch (e) {\r\n        console.error(e);\r\n        if (e.response.status === 401) {\r\n          this.$handle_session_expired()\r\n        }\r\n      }\r\n    },\r\n\r\n    createLogicFromData(list) {\r\n      if (list.length > 0) {\r\n        const obj = {days: {}};\r\n        for (let i = 0; i < list.length; i++) {\r\n          obj.days[list[i].day + \"/\" + list[i].month] = {lessons: []};\r\n        }\r\n        for (let i = 0; i < list.length; i++) {\r\n          let listTmp = obj.days[list[i].day + \"/\" + list[i].month].lessons;\r\n          listTmp.push(list[i]);\r\n        }\r\n        this.dataLogic = obj;\r\n        console.log(obj);\r\n      }else{\r\n        this.dataLogic = null;\r\n      }\r\n    },\r\n\r\n\r\n    handleSubmitLesson(index, item) {\r\n      this.isPopUpOpen = true;\r\n      this.dataToSend = item;\r\n    },\r\n\r\n    closePopUp() {\r\n      this.isPopUpOpen = false;\r\n    },\r\n\r\n    opencloseFeedBack(flag) {\r\n      this.isFeedBackOpen = flag;\r\n    },\r\n\r\n    async SendData(state) {\r\n      try {\r\n        let response;\r\n        let dataToSend = toRaw(this.dataToSend);\r\n        if (state === 2) {\r\n          response = axios.post('ServletSetBookings',null,{params : {action : 'web',email_professore : dataToSend.professor.email,corso : dataToSend.course.course_titol,giorno : dataToSend.day,mese : dataToSend.month,orario : dataToSend.hour,stato : state,}});\r\n        } else if (state === 3) {\r\n          response = axios.post('ServletSetBookings',null,{params : {action : 'web',email_professore : dataToSend.professor.email,corso : dataToSend.course.course_titol,giorno : dataToSend.day,mese : dataToSend.month,orario : dataToSend.hour,stato : state,}})\r\n        }\r\n        console.log(\"AGGIORNO\");\r\n        await this.fetchData();\r\n      } catch (e) {\r\n        console.error(e.response)\r\n      }\r\n    }\r\n\r\n\r\n  },\r\n\r\n  watch: {\r\n    listOfReservedLessons(newList, oldList) {\r\n      this.createLogicFromData(toRaw(newList));\r\n    },\r\n\r\n    selectedOptions(newValue,oldValue){\r\n      this.state = newValue.index\r\n      this.fetchData();\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    orderListOfDays() {\r\n      if(this.dataLogic){\r\n        return Object.keys(this.dataLogic.days).sort();\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n\r\n.my-container {\r\n  height: 200px;\r\n  border: solid 1px black;\r\n}\r\n\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAsD;;EAI5DA,KAAK,EAAC;AAAK;;;EAcJA,KAAK,EAAC;;gCAChBC,mBAAA,CAA0B,WAAvB,qBAAmB;oBAAtBC,UAA0B,C;;;;;6DAnB5BD,mBAAA,CAGM,OAHNE,UAGM,GAFJC,YAAA,CAC2CC,yBAAA;IAD3BC,KAAwB,EAAxB;MAAA;IAAA,CAAwB;IAAEC,YAAY,EAAE,IAAI;IAAGC,UAAU,EAAE,KAAK;IAAGC,OAAO,EAAEC,KAAA,CAAAD,OAAO;IAAEE,KAAK,EAAC,OAAO;gBACzFD,KAAA,CAAAE,eAAe;+DAAfF,KAAA,CAAAE,eAAe,GAAAC,MAAA;wDAEnBH,KAAA,CAAAI,SAAS,I,kBAAhCC,mBAAA,CAaMC,SAAA;IAAAC,GAAA;EAAA,GAAAC,WAAA,CAbmDC,QAAA,CAAAC,eAAe,GAA9BC,IAAI,EAACC,KAAK;yBAApDP,mBAAA,CAaM,OAbNQ,UAaM,GAXRC,mBAAA,sDAAuD,EACvDA,mBAAA,4BAA+B,EAC/BA,mBAAA,sEAAuE,EACvEA,mBAAA,uFAAoF,EACpFA,mBAAA,oGAAqG,EACrGA,mBAAA,cAAiB,EACjBA,mBAAA,cAAiB,C;oDAMfT,mBAAA,CAEM,OAFNU,UAEM,EAAAC,UAAA,IACahB,KAAA,CAAAiB,WAAW,IAAIjB,KAAA,CAAAkB,KAAK,U,cAAvCC,YAAA,CAA4HC,sBAAA;;IAA5E,wBAAsB,EAAEX,QAAA,CAAAY,UAAU;IAAG,4BAA0B,EAAEZ,QAAA,CAAAa;0HAC5FtB,KAAA,CAAAuB,cAAc,I,cAAnCJ,YAAA,CAC2EK,wBAAA;;IADrC,cAAY,EAAE,cAAc;IAClD,wBAAsB,EAAEf,QAAA,CAAAgB"},"metadata":{},"sourceType":"module","externalDependencies":[]}