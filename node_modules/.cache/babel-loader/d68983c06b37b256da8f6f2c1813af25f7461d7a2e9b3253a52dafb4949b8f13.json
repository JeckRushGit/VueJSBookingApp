a(toRaw(newList));\n    }\n  },\n  computed: {\n    orderListOfDays() {\n      return Object.keys(this.dataLogic.days).sort();\n    }\n  }\n};","map":{"version":3,"names":["axios","toRaw","AgendaLessonContainer","FeedBackPopUp","AgendaPopUp","name","components","data","state","listOfReservedLessons","dataLogic","isPopUpOpen","isFeedBackOpen","dataToSend","created","fetchData","methods","response","get","params","action","stato","e","console","error","status","$handle_session_expired","createLogicFromData","list","length","obj","days","i","day","month","lessons","listTmp","push","log","handleSubmitLesson","index","item","closePopUp","opencloseFeedBack","flag","watch","newList","oldList","computed","orderListOfDays","Object","keys","sort"],"sources":["C:\\Users\\giaco\\Desktop\\folderperview\\src\\pages\\AgendaPage.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"dataLogic\" v-for=\"(item,index) in orderListOfDays \">\r\n    <div class=\"my-container m-2 overflow-auto\">\r\n      <p>{{ item }} </p>\r\n      <p v-for=\"(item2,index2) in dataLogic.days[item].lessons\">\r\n        <AgendaLessonContainer :index=\"index2\" :item=\"item2\"\r\n                               :call-back-lesson=\"handleSubmitLesson\"></AgendaLessonContainer>\r\n      </p>\r\n    </div>\r\n  </div>\r\n  <AgendaPopUp v-if=\"isPopUpOpen\" :call-back-close-pop-up=\"closePopUp\"/>\r\n  <FeedBackPopUp v-if=\"isFeedBackOpen\" :teleport-tag=\"'#agendapopup'\" :call-back-close-pop-up=\"opencloseFeedBack\"></FeedBackPopUp>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport {toRaw} from \"vue\";\r\nimport AgendaLessonContainer from \"@/components/AgendaLessonContainer.vue\";\r\nimport FeedBackPopUp from \"@/components/FeedBackPopUp.vue\";\r\nimport AgendaPopUp from \"@/components/AgendaPopUp.vue\";\r\n\r\nexport default {\r\n  name: \"AgendaPage\",\r\n  components: {AgendaPopUp, FeedBackPopUp, AgendaLessonContainer},\r\n  data() {\r\n    return {\r\n      state: 1,\r\n      listOfRes